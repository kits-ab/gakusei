{
  "name": "gakusei",
  "description": "J-App",
  "private": true,
  "author": "Kits AB",
  "license": "MIT",
  "homepage": "https://github.com/kits-ab/gakusei",
  "bugs": {
    "url": "https://github.com/kits-ab/gakusei/issues"
  },
  "scripts": {
    "precompile": "rimraf ./src/main/resources/static/js/*",
    "compile:main": "browserify ./src/main/js/main.js --debug -t babelify -o ./src/main/resources/static/js/main_bundle.js",
    "compile:login": "browserify ./src/main/js/login.js --debug -t babelify -o ./src/main/resources/static/js/login_bundle.js",
    "compile": "npm run compile:login && npm run compile:main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kits-ab/gakusei.git"
  },
  "dependencies": {
    "browserify": "^13.1.1",
    "hyphenation.sv": "^0.2.1",
    "hypher": "^0.2.5",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "whatwg-fetch": "^2.0.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.1",
    "rimraf": "^2.5.2"
  }
}
