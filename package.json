  },
  "scripts": {
    "compile": "webpack --progress --profile --colors --hide-modules",
    "compile:production": "cross-env NODE_ENV=production webpack --colors --hide-modules",
    "precommit": "lint-staged",
    "test": "./node_modules/.bin/mocha --opts ./mocha.opts",
    "start": "webpack-dev-server",
    "lint": "npm run -s lint-js && npm run -s lint-js && npm run -s lint-css && npm run -s lint-css",
    "lint-js": "./node_modules/.bin/prettier-eslint --write \"src/main/js/**/*.{js,jsx}\"",
    "lint-js-staged": "./node_modules/.bin/prettier-eslint --print-width=120 --write",
    "lint-css": "./node_modules/.bin/prettier-stylelint --write --quiet \"src/main/resources/static/css/**/*.css\"",
    "lint-css-staged": "./node_modules/.bin/prettier-stylelint --write \"*.css\""
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint-js-staged",
      "git add"
    ],
    "*.css": [
      "npm run lint-css-staged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kits-ab/gakusei.git"
  },
