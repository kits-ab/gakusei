    "formsy-react-components": "^0.10.1",
    "hyphenation.sv": "^0.2.1",
    "hypher": "^0.2.5",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-chartjs-2": "^2.0.0",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
